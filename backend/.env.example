# Backend environment example
# Copy to .env and adjust values

# Server
PORT=5000

# Database
MONGODB_URI=mongodb://localhost:27017/rewash

# Optional: Proxy ML FastAPI service (e.g., http://127.0.0.1:8010)
FASTAPI_URL=

# When set to true (or in NODE_ENV=test) the backend will not attempt to
# reach the FastAPI predictor or execute local python scripts. Useful for CI
# and local dev where the predictor is intentionally separate.
SKIP_STOCK_INTEGRATION=true
